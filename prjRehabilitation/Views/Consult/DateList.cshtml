@model IEnumerable<prjRehabilitation.ViewModel.CConsultationViewModel>

@{
    ViewData["Title"] = "DateList";
}

<h1>@ViewBag.name 會談紀錄</h1>

<p>
    <a asp-action="Create">新增會談紀錄</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                序號
            </th>

            <th>
                @Html.DisplayNameFor(model => model.Date)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Summary)
            </th>

            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.FConsultId)
                </td>

                <td>
                    @Html.DisplayFor(modelItem => item.Date)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Summary)
                </td>

                <td>
                    @Html.ActionLink("編輯", "Edit", new {  id=item.FConsultId }) |
                    @Html.ActionLink("查看", "Details", new { id=item.FConsultId }) |
                    @Html.ActionLink("刪除", "Delete", new {  id=item.FConsultId })
                </td>
            </tr>
        }
    </tbody>
</table>
