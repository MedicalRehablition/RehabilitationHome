    @await RenderSectionAsync("Scripts", required: false)
    <script>
        const LoginA = document.querySelector(".LoginA");
        //========抓使用者===============
        async function GetUser() {

            const response = await fetch("@Url.Content("/AdminUserLogin/GetUserSession")");
            const User = await response.json();
            if (User != "") {
                const span = document.querySelector(".AdminUserName");
                const LoginA = document.querySelector(".LoginA");
                const AdminUserImg = document.querySelector(".AdminUserImg");
                $(".LoginSpan").text("登出")
                span.innerText = User.fName;
                $(".AdminUserImg").attr("src", "/images/" + User.fphoto)
            }

        }
        GetUser();
        //==========清除使用者=========================
        LoginA.addEventListener("click", function () {
            const xhr = new XMLHttpRequest();
            xhr.addEventListener("load", function () {
                if (xhr.responseText == "清除session") {
                    $(".LoginSpan").text("登入")
                }
            })
            xhr.open("Post", "@Url.Content("/AdminUserLogin/RemoveUserSession")");
            xhr.send();
        })

    </script>
