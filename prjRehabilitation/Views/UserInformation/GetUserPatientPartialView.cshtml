@model IEnumerable<prjRehabilitation.ViewModel.CPatientsViewModel>
@{
    Layout = "_Layout";
}
<div class="container">
    <div class="row">
        @{
            int count = 0;
            foreach (var item in Model)
            {
                        <div class="col-md-6 col-lg-4 g-mb-30" style="padding:20px;">
                            <article class="u-shadow-v18 g-bg-white text-center rounded g-px-20 g-py-40 g-mb-5">
                                <img src="~/images/@item.photo" width="300px" height="300px" />
                                <span class="d-block g-color-primary g-font-size-16">$@Html.DisplayFor(modelItem => item.FName)</span>
                                <!-- 只有上部圓角 -->
                                <div class="card-body">
                                    <h4 class="h5 g-color-black g-font-weight-600 g-mb-10">@Html.DisplayFor(modelItem => item.PatientGetMedDates)</h4>
                            @Html.ActionLink("編輯商品", "Edit", new {  id=item.Fid })
                            @Html.ActionLink("刪除商品", "Delete", new {  id=item.Fid },new { @onclick = "return confirm('是否刪除?')"})
                                </div>
                            </article>
                        </div>
            }
        }
    </div>
</div>
<h2>hello</h2>
